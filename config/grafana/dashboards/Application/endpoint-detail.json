{
  "uid": "endpoint-detail",
  "title": "L3: Endpoint Detail",
  "description": "Endpoint-level deep dive with RED metrics filtered by URI, using Native Histograms",
  "tags": ["spring-boot", "endpoint", "native-histogram"],
  "timezone": "browser",
  "editable": true,
  "refresh": "15s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "links": [
    {
      "title": "L0: Application Health",
      "type": "link",
      "icon": "arrow-left",
      "url": "/d/application-health?${datasource:queryparam}&var-job=${job}&var-instance=${instance}",
      "tooltip": "Back to Application Health overview"
    },
    {
      "title": "L1: Service RED Overview",
      "type": "link",
      "icon": "arrow-left",
      "url": "/d/service-red-overview?${datasource:queryparam}&var-job=${job}&var-instance=${instance}",
      "tooltip": "Back to Service Overview"
    },
    {
      "title": "L4: Debug / Trace",
      "type": "link",
      "icon": "search",
      "url": "/d/debug-trace?var-metrics_ds=${datasource}&var-job=${job}&var-service=${job}",
      "tooltip": "Go to Trace Search"
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "label": "Metrics",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "selected": true,
          "text": "Mimir-Staging",
          "value": "mimir-staging"
        }
      },
      {
        "name": "job",
        "type": "query",
        "datasource": { "uid": "${datasource}" },
        "query": "label_values(http_server_requests_milliseconds, job)",
        "refresh": 2,
        "sort": 1,
        "current": {
          "selected": true,
          "text": "caro-demo-app",
          "value": "caro-demo-app"
        }
      },
      {
        "name": "instance",
        "label": "Instance",
        "type": "query",
        "datasource": { "uid": "${datasource}" },
        "query": "label_values(http_server_requests_milliseconds{job=\"$job\"}, instance)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "method",
        "label": "Method",
        "type": "query",
        "datasource": { "uid": "${datasource}" },
        "query": "label_values(http_server_requests_milliseconds{job=\"$job\"}, method)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "uri",
        "type": "query",
        "datasource": { "uid": "${datasource}" },
        "query": "label_values(http_server_requests_milliseconds{job=\"$job\"}, uri)",
        "refresh": 2,
        "sort": 1,
        "includeAll": false,
        "current": {
          "selected": false,
          "text": "",
          "value": ""
        }
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Summary",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Request Rate",
      "description": "Current requests per second for selected endpoint",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "type": "stat",
      "title": "5xx Error Rate",
      "description": "HTTP 5xx server error percentage for selected endpoint",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "expr": "(\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\",status=~\"5..\"}[$__rate_interval]))) or vector(0))\n  /\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval]))) > 0)\n) * 100",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "noValue": "0"
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "type": "stat",
      "title": "4xx Client Error Rate",
      "description": "HTTP 4xx client error percentage for selected endpoint",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "(\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\",status=~\"4..\"}[$__rate_interval]))) or vector(0))\n  /\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval]))) > 0)\n) * 100",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 15 }
            ]
          },
          "noValue": "0"
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "type": "stat",
      "title": "P95 Latency",
      "description": "95th percentile response time for selected endpoint",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "type": "stat",
      "title": "SLO Compliance",
      "description": "Requests under 500ms (SLO target: 99%)",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "(histogram_fraction(0, 500, sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval]))) * 100) or vector(100)",
          "legendFormat": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "type": "row",
      "title": "Rate",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Request Rate Trend",
      "description": "Requests per second over time for selected endpoint",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "$uri"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Status Code Distribution",
      "description": "Requests per second by HTTP status code for selected endpoint",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "sum by (status) (histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "{{ status }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "2.." },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "4.." },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "5.." },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "row",
      "title": "Errors",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "5xx Error Rate Trend",
      "description": "HTTP 5xx server error percentage over time for selected endpoint",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "targets": [
        {
          "expr": "(\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\",status=~\"5..\"}[$__rate_interval]))) or vector(0))\n  /\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval]))) > 0)\n) * 100",
          "legendFormat": "5xx Error Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "thresholdsStyle": { "mode": "line" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 5 }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Error Count by Status",
      "description": "4xx and 5xx error count per second by status code",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "targets": [
        {
          "expr": "sum by (status) (histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\",status=~\"[45]..\"}[$__rate_interval])))",
          "legendFormat": "{{ status }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "4.." },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byRegexp", "options": "5.." },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "4xx Client Error Rate Trend",
      "description": "HTTP 4xx client error percentage over time (auth, validation, not found)",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 23 },
      "targets": [
        {
          "expr": "(\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\",status=~\"4..\"}[$__rate_interval]))) or vector(0))\n  /\n  (sum(histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval]))) > 0)\n) * 100",
          "legendFormat": "4xx Error Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "thresholdsStyle": { "mode": "line" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 30 }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "4xx Client Error Count by Status",
      "description": "4xx client error count per second broken down by status code",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 23 },
      "targets": [
        {
          "expr": "sum by (status) (histogram_count(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\",status=~\"4..\"}[$__rate_interval])))",
          "legendFormat": "{{ status }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "stacking": { "mode": "normal" }
          },
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "row",
      "title": "Duration",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Latency Percentiles (P50 / P95 / P99)",
      "description": "Response time percentiles over time for selected endpoint. Click exemplar dots to jump to the corresponding trace.",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "P50",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "P95",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "P99",
          "exemplar": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 5,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "P50" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "P95" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "P99" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "last"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "timeseries",
      "title": "Latency Overview (Avg vs Percentiles)",
      "description": "Average latency compared to percentile lines. Click exemplar dots to jump to the slow traces.",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "targets": [
        {
          "expr": "histogram_avg(sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "Average",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "P95",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval])))",
          "legendFormat": "P99",
          "exemplar": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Average" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } },
              { "id": "custom.lineWidth", "value": 2 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "P95" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } },
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "P99" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } },
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } }
            ]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "last"] },
        "tooltip": { "mode": "multi", "sort": "desc" }
      }
    },
    {
      "type": "heatmap",
      "title": "Latency Distribution Heatmap",
      "description": "Response time distribution for selected endpoint. Reveals bimodal distributions, tail latency spikes, and outliers that are invisible in percentile lines.",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 40 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_milliseconds{job=\"$job\",instance=~\"$instance\",method=~\"$method\",uri=\"$uri\"}[$__rate_interval]))",
          "legendFormat": "",
          "format": "heatmap"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      },
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "scheme",
          "reverse": false,
          "scale": "exponential",
          "scheme": "Oranges",
          "steps": 64
        },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-9 },
        "tooltip": { "mode": "single", "yHistogram": false },
        "yAxis": {
          "axisPlacement": "left",
          "reverse": false,
          "unit": "ms"
        }
      }
    },
    {
      "type": "row",
      "title": "Comparison",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 48 },
      "collapsed": false
    },
    {
      "type": "table",
      "title": "Endpoint Comparison",
      "description": "All endpoints with Rate, Error %, P50, P95, P99 - compare selected endpoint against others",
      "datasource": { "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 49 },
      "targets": [
        {
